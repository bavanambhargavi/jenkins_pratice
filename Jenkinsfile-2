pipeline {  
    agent any  
    stages {  
            stage ('Build') {  
                echo "Build this job" 
            }  
            stage ('Test') {
                echo "testing the job"  
                 
            }  
            stage ('QA') { 
               echo 'pip install pytest' 
            }  
            stage ('Deploy') {  
                echo "Deployement" 
            }  
            stage ('Monitor') {  
                echo "Monitoring the job" 
            }
    }
}  